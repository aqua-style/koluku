  <div class="container">
      <%= render 'layouts/flash_messages' %>
      
      <div class="search">
        <div class="row m5-r m5-l">
          <div class="text-center">
            <%= form_tag(shops_path, method: :get) do %>
              <div class="form-group form-inline">
